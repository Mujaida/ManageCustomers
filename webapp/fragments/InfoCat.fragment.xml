		items="{path:'creditInfoSet>/'}" search="handleValueHelpInfoCatSearch"
		confirm="handleValueHelpInfoCatConfirm" cancel="handleValueHelpInfoCatClose" multiSelect="false">
		<StandardListItem title="{creditInfoSet>InformationCat}" description="{creditInfoSet>InformationDes}" />
	</SelectDialog>
</core:FragmentDefinition>